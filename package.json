{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "hue",
  "title": "Hue",
  "description": "Quickly and easily control Philips Hue lights",
  "icon": "command-icon.png",
  "author": "pindab0ter",
  "license": "MIT",
  "commands": [
    {
      "name": "connect-to-hue-bridge",
      "title": "Connect to Hue Bridge",
      "description": "Template for showing a static list",
      "mode": "view"
    }
  ],
  "dependencies": {
    "@raycast/api": "^1.38.2",
    "@raycast/utils": "^1.0.7",
    "node-hue-api": "^5.0.0-beta.13"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "prettier": "^2.5.1",
    "typescript": "^4.4.3"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop",
    "fix-lint": "ray lint --fix",
    "lint": "ray lint",
    "publish": "ray publish"
  }
}
